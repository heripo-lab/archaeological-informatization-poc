        {
          "name": "grid_system_desc",
          "type": "string",
          "description": "격자 또는 트렌치를 설정한 방식에 대한 설명입니다."
        },
        {
          "name": "excavation_depth_max",
          "type": "number",
          "description": "최대 굴착 깊이(m)입니다."
        },
        {
          "name": "remarks",
          "type": "string",
          "description": "기타 참고사항 또는 조사 특이사항입니다."
        },
        {
          "name": "images",
          "type": "string[]",
          "description": "조사 유적 관련 이미지 URL들을 담은 JSON 문자열 배열입니다."
        }
      ]
    },
